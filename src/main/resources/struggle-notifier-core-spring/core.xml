<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:context="http://www.springframework.org/schema/context"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.5.xsd"
	   default-autowire="byName">

	<context:property-placeholder
			ignore-resource-not-found="true"
			ignore-unresolvable="true"
			location="classpath:runtimecfg/rabbitmq-config.properties" />

	<context:component-scan base-package="com.houlu.util.test.service.*" />

	<import resource="classpath:/struggle-notifier-core-spring/spring-rabbitmq.xml" />


</beans>